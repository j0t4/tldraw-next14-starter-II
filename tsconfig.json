{
  //"extends": "./tsconfig_base.json",
  "include": [
    "src",
    "scripts",
    "vite.config.mts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    //"src/server",
    "node_modules",
    "dist",
    ".tsbuild*"
  ],
  "compilerOptions": {
    "noEmit": true,
    "emitDeclarationOnly": false,
    "target": "es2015",
    "allowJs": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "strict": false,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "references": []
}
